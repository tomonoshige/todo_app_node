<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>TODOアプリケーション</title>
    <link rel="stylesheet" href="/css/style.css">
  </head>
  <body>
    <%- include('header') %>

    <div class="top-wrapper">
      <div class="container">
        <a href="/index" class="rein">一覧に戻る⬅︎</a>
        <div>
          <% howto_todo.forEach((howto, index) => { %>
          <p class="todo-list"><%= howto.title %></p>
          <p><%= howto.article %></p>
          <% }); %>
        </div>
      </div>
    </div>
  </body>
</html>
